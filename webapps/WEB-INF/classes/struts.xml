<struts>

	<package name="ah-default" extends="tiles-default">
		<interceptors>
			<interceptor name="accessControl"
				class="com.ah.ui.interceptors.AccessControlInterceptor" />
			<interceptor name="globalForward"
				class="com.ah.ui.interceptors.GlobalForwardInterceptor" />
			<interceptor name="sessionTimeOut"
				class="com.ah.ui.interceptors.SessionInterceptor" />
			<interceptor name="hmFileUpload"
				class="com.ah.ui.interceptors.HmFileUploadInterceptor" />

			<interceptor-stack name="ahStack">
				<interceptor-ref name="sessionTimeOut" />
				<interceptor-ref name="hmFileUpload" />
				<interceptor-ref name="defaultStack" />
				<interceptor-ref name="accessControl" />
				<interceptor-ref name="globalForward" />
			</interceptor-stack>

			<interceptor-stack name="accessStack">
				<interceptor-ref name="defaultStack" />
				<interceptor-ref name="accessControl" />
			</interceptor-stack>
		</interceptors>

		<default-interceptor-ref name="ahStack" />

		<global-results>
			<!-- Landing page -->
			<result type="redirectAction">
				<param name="actionName">topMenu</param>
				<param name="operation">${operation}</param>
			</result>
			<result name="login" type="redirectAction">
				<param name="actionName">login</param>
			</result>
			<result name="license" type="redirectAction">
				<param name="actionName">license</param>
				<param name="operation">license</param>
			</result>
			<result name="eula" type="redirectAction">
				<param name="actionName">license</param>
				<param name="operation">eula</param>
			</result>
			<result name="startHere" type="redirectAction">
				<param name="actionName">startHere</param>
			</result>
			<result name="ssl" type="redirectAction">
				<param name="actionName">ssl</param>
			</result>
			<result name="deny" type="redirectAction">
				<param name="actionName">deny</param>
			</result>
			<result name="baseRedirect" type="redirect">
				<param name="location">${baseRedirectUrl}</param>
			</result>

			<result name="error503">/tiles/err503.jsp</result>

			<result name="json">/tiles/json.jsp</result>
		</global-results>
	</package>

	<package name="global" extends="ah-default" namespace="/">

		<default-interceptor-ref name="accessStack" />
         <global-results>
			<result name="updateSoftware" type="redirectAction">
				<param name="actionName">updateSoftware</param>
			</result>
         </global-results>
		<!-- HM access deny -->
		<action name="deny" class="com.ah.ui.actions.LoginAction"
			method="deny">
			<result type="redirectAction" name="blankPage">blank</result>
			<result type="tiles" name="denyPage">denyPage</result>
			<interceptor-ref name="defaultStack" />
		</action>

		<action name="blank" class="com.ah.ui.actions.LoginAction"
			method="blank">
			<result type="tiles" name="blankPage">blankPage</result>
			<interceptor-ref name="defaultStack" />
		</action>

		<!-- SSL -->
		<action name="ssl" class="com.ah.ui.actions.LoginAction"
			method="ssl">

			<result type="tiles" name="ssl">ssl</result>
		</action>

		<!-- Login -->
		<action name="login" class="com.ah.ui.actions.LoginAction"
			method="login">

			<result type="tiles" name="login">login</result>
			<result type="tiles" name="ssl">ssl</result>
			<result name="redirect" type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>
			<!-- Welcome Page -->
			<result name="startHere" type="redirectAction">
				<param name="actionName">startHere</param>
			</result>

			<!-- end user license agreement -->
			<result type="tiles" name="eula">eula</result>
		</action>
		
		<!-- recreate user info for MyHive customer user -->
		<action name="recreateUser" class="com.ah.ui.actions.LoginAction"
			method="checkUserAndRedirect">
			<result name="redirect" type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>
			<!-- Welcome Page -->
			<result name="startHere" type="redirectAction">
				<param name="actionName">startHere</param>
			</result>

			<!-- end user license agreement -->
			<result type="tiles" name="eula">eula</result>
		</action>

		<!-- Logout -->
		<action name="logout" class="com.ah.ui.actions.LoginAction"
			method="logout">
			<result type="tiles" name="login">login</result>
			<result name="redirect" type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>
		</action>

		<!-- Authenticate -->
		<action name="authenticate" class="com.ah.ui.actions.LoginAction"
			method="authenticate">

			<!-- Login failed, return to login page -->
			<result type="tiles" name="login">login</result>

			<!-- Welcome Page -->
			<result name="startHere" type="redirectAction">
				<param name="actionName">startHere</param>
			</result>

			<!-- end user license agreement -->
			<result type="tiles" name="eula">eula</result>

			<!-- passive node redirect to active node -->
			<result name="redirect" type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>
		</action>

		<action name="mcs" class="com.ah.ui.actions.tools.MCSAction">
		</action>

		<!-- License -->
		<action name="license" class="com.ah.ui.actions.LoginAction"
			method="license">
			<result type="tiles" name="license">license</result>

			<!-- session expired -->
			<result type="tiles" name="login">login</result>

			<result name="redirect" type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>

			<!-- Welcome Page -->
			<result name="startHere" type="redirectAction">
				<param name="actionName">startHere</param>
			</result>

			<!-- end user license agreement -->
			<result type="tiles" name="eula">eula</result>
		</action>

		<!-- Switch Domain -->
		<action name="switchDomain" class="com.ah.ui.actions.SwitchDomainAction">
			<result name="json">/tiles/json.jsp</result>
			<!-- Recreate User -->
			<result name="recreateUser" type="redirectAction">
				<param name="actionName">recreateUser</param>
			</result>
			<result type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>
		</action>
		
		<!-- Redirect Between Application  -->
		<action name="appRedirect" class="com.ah.ui.actions.AppRedirectAction">
			<result type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>
		</action>

		<!-- Push -->
		<action name="push" class="com.ah.ui.actions.PushAction">
			<result>/tiles/push.jsp</result>
			<result type="tiles" name="login">login</result>
		</action>

		<!-- Search -->
		<action name="search" class="com.ah.ui.actions.SearchAction">
			<result name="json">/tiles/json.jsp</result>
		</action>

		<action name="helpSetting" class="com.ah.ui.actions.HelpSettingAction">
			<result name="json">/tiles/json.jsp</result>
		</action>

		<!-- Welcome Page -->
		<action name="startHere" class="com.ah.ui.actions.LoginAction"
			method="startHere">
			<result type="tiles" name="welcomePage">welcomePage</result>

			<result type="tiles" name="license">license</result>

			<result name="json">/tiles/json.jsp</result>
			
			<!-- session expired -->
			<result type="tiles" name="login">login</result>

			<result name="redirect" type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>
			
			<result name="newWelcomePageAction" type="redirectAction">
				<param name="actionName">newStartHere</param>
			</result>
		</action>
		<action name="newStartHere" class="com.ah.ui.actions.LoginAction"
			method="newStartHere">
			<result type="tiles" name="welcomePage">newWelcomePage</result>
			<result type="tiles" name="welcomePageDone">newWelcomePageDone</result>
			<result type="tiles" name="license">license</result>
			<result name="json">/tiles/json.jsp</result>
			<!-- session expired -->
			<result type="tiles" name="login">login</result>
			<result name="redirect" type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>
		</action>

		<!-- Teacher View -->
		<action name="teacherView"
			class="com.ah.ui.actions.teacherView.TvRedirectionAction">
			<result type="tiles">selectClass</result>
			<result name="json">/tiles/json.jsp</result>
			<result name="redirect" type="tiles">tvRedirect</result>
			<result name="redirectLogin" type="redirect">
				<param name="location">${redirectUrl}</param>
			</result>
		</action>

		<!-- Student registration -->
		<action name="studentRegist"
			class="com.ah.ui.actions.teacherView.StudentRegisterAction">
			<result type="tiles" name="registration">registration</result>
			<result type="tiles" name="registSuccess">registSuccess</result>
			<result name="json">/tiles/json.jsp</result>
		</action>

		<action name="randValidateCode" class="com.ah.ui.actions.teacherView.RandomAction">
			<result type="stream">
				<param name="contentType">image/jpeg</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>

		<!-- Notification Message -->
        <action name="notificationMsg" class="com.ah.ui.actions.NotificationMsgAction">
            <result name="json">/tiles/json.jsp</result>
        </action>
	</package>

