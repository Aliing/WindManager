<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:general="http://www.aerohive.com/configuration/general" xmlns:aaa="http://www.aerohive.com/configuration/aaa" targetNamespace="http://www.aerohive.com/configuration/aaa">
	<xsd:import namespace="http://www.aerohive.com/configuration/general" schemaLocation="general.xsd"/>
	<xsd:complexType name="aaa-auth-port">
		<xsd:attribute name="value" type="general:ah-port-value" use="optional" default="1812"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="aaa-acct-port">
		<xsd:attribute name="value" type="general:ah-port-value" use="optional" default="1813"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-tls-port">
		<xsd:attribute name="value" type="general:ah-port-value" use="optional" default="2083"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-server-type">
		<xsd:sequence>
			<xsd:element name="old-shared-secret" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="auth-port" minOccurs="0">
							<xsd:complexType>
								<xsd:attribute name="value" use="optional" default="1812">
									<xsd:simpleType>
										<xsd:restriction base="xsd:int">
											<xsd:minInclusive value="1"/>
											<xsd:maxInclusive value="65535"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:attribute>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="acct-port" minOccurs="0">
							<xsd:complexType>
								<xsd:attribute name="value" use="optional" default="0">
									<xsd:simpleType>
										<xsd:restriction base="xsd:int">
											<xsd:minInclusive value="0"/>
											<xsd:maxInclusive value="65535"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:attribute>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
					<xsd:attribute name="value" type="xsd:string" use="required"/>
					<xsd:attribute name="encrypted" type="general:ah-encrypted-value" use="optional" default="1"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="shared-secret" type="general:ah-encrypted-string-act" minOccurs="0"/>
			<xsd:element name="auth-port" type="aaa:aaa-auth-port" minOccurs="0"/>
			<xsd:element name="acct-port" type="aaa:aaa-acct-port" minOccurs="0"/>
			<xsd:element name="via-vpn-tunnel" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-accounting-server">
		<xsd:sequence>
			<xsd:element name="shared-secret" type="general:ah-encrypted-string-act" minOccurs="0"/>
			<xsd:element name="acct-port" type="aaa:aaa-acct-port" minOccurs="0"/>
			<xsd:element name="via-vpn-tunnel" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="STA-auth-private-key">
		<xsd:sequence>
			<xsd:element name="private-key-password" type="general:ah-encrypted-string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="STA-auth-server-cert">
		<xsd:sequence>
			<xsd:element name="private-key" type="aaa:STA-auth-private-key"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="STA-auth-ca-cert">
		<xsd:sequence>
			<xsd:element name="server-cert" type="aaa:STA-auth-server-cert"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="STA-auth-tls">
		<xsd:sequence>
			<xsd:element name="cr" type="xsd:string" fixed="" minOccurs="0"/>
			<xsd:element name="check-cert-cn" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="check-in-db" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="STA-auth-checks">
		<xsd:sequence>
			<xsd:element name="AH-DELTA-ASSISTANT" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="check-in-db" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="STA-auth-positive-type">
		<xsd:sequence>
			<xsd:element name="peap" type="aaa:STA-auth-checks" minOccurs="0"/>
			<xsd:element name="ttls" type="aaa:STA-auth-checks" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="LDAP-auth-verify-server-value">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="demand"/>
			<xsd:enumeration value="never"/>
			<xsd:enumeration value="try"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="LDAP-auth-private-key-password">
		<xsd:sequence>
			<xsd:element name="verify-server" minOccurs="0">
				<xsd:complexType>
					<xsd:attribute name="value" type="aaa:LDAP-auth-verify-server-value" use="optional" default="try"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
		<xsd:attribute name="encrypted" type="general:ah-encrypted-value" use="optional" default="1"/>
	</xsd:complexType>
	<xsd:complexType name="LDAP-auth-private-key">
		<xsd:sequence>
			<xsd:element name="private-key-password" type="aaa:LDAP-auth-private-key-password"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="LDAP-auth-client-cert">
		<xsd:sequence>
			<xsd:element name="private-key" type="aaa:LDAP-auth-private-key"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="LDAP-auth-ca-cert">
		<xsd:sequence minOccurs="0">
			<xsd:element name="client-cert" type="aaa:LDAP-auth-client-cert"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="LDAP-auth-primary">
		<xsd:sequence>
			<xsd:element name="type" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="tls">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="ca-cert" type="aaa:LDAP-auth-ca-cert"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="active-directory-user">
		<xsd:sequence>
			<xsd:element name="password" type="general:ah-encrypted-string"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="active-directory-login">
		<xsd:sequence>
			<xsd:element name="user" type="aaa:active-directory-user"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="active-directory-workgroup">
		<xsd:sequence>
			<xsd:element name="realm" type="general:ah-string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="active-directory-server">
		<xsd:sequence>
			<xsd:element name="cr" type="xsd:string" fixed="" minOccurs="0"/>
			<xsd:element name="login" type="aaa:active-directory-login" minOccurs="0"/>
			<xsd:element name="via-vpn-tunnel" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="active-directory-login-admin">
		<xsd:sequence>
			<xsd:element name="admin-user" type="aaa:active-directory-user" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="active-directory-domain">
		<xsd:sequence>
			<xsd:element name="server" type="general:ah-string-act" minOccurs="0"/>
			<xsd:element name="fullname" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="default" type="general:ah-act-show" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="value" type="xsd:string" use="required"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="basedn" type="general:ah-string-act" minOccurs="0"/>
			<xsd:element name="binddn" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="password" type="general:ah-encrypted-string" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="value" type="xsd:string" use="required"/>
					<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act-value" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="active-directory-primary">
		<xsd:sequence>
			<xsd:element name="global-catalog" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="server" type="aaa:active-directory-server" minOccurs="0"/>
			<xsd:element name="computer-ou" type="general:ah-string-act" minOccurs="0"/>
			<xsd:element name="login" type="aaa:active-directory-login-admin" minOccurs="0"/>
			<xsd:element name="domain" type="aaa:active-directory-domain" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="tls-enable" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="sasl-wrapping" type="general:ah-string-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="ldap-server-identity">
		<xsd:sequence>
			<xsd:element name="password" type="general:ah-encrypted-string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="ldap-server-protocol-value">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ldap"/>
			<xsd:enumeration value="ldaps"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="ldap-server-protocol">
		<xsd:attribute name="value" type="aaa:ldap-server-protocol-value" default="ldap"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="ldap-server-port">
		<xsd:attribute name="value" use="optional">
			<xsd:simpleType>
				<xsd:restriction base="xsd:int">
					<xsd:minInclusive value="1"/>
					<xsd:maxInclusive value="65535"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="ldap-server-name">
		<xsd:sequence>
			<xsd:element name="via-vpn-tunnel" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="aaa-ldap-server">
		<xsd:sequence>
			<xsd:element name="server" type="aaa:ldap-server-name" minOccurs="0"/>
			<xsd:element name="filter-attr" type="general:ah-string-act" minOccurs="0"/>
			<xsd:element name="old-filter_attr" type="general:ah-string-act" minOccurs="0"/>
			<xsd:element name="basedn" type="general:ah-string-act" minOccurs="0"/>
			<xsd:element name="binddn" type="aaa:ldap-server-identity" minOccurs="0"/>
			<xsd:element name="protocol" type="aaa:ldap-server-protocol" minOccurs="0"/>
			<xsd:element name="port" type="aaa:ldap-server-port" minOccurs="0"/>
			<xsd:element name="no-strip-filter" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="local-retry-interval">
		<xsd:attribute name="value" use="optional" default="600">
			<xsd:simpleType>
				<xsd:restriction base="xsd:int">
					<xsd:minInclusive value="60"/>
					<xsd:maxInclusive value="200000000"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-retry-interval">
		<xsd:attribute name="value" use="optional" default="600">
			<xsd:simpleType>
				<xsd:restriction base="xsd:int">
					<xsd:minInclusive value="60"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-account-interim-interval">
		<xsd:attribute name="value" use="optional" default="20">
			<xsd:simpleType>
				<xsd:restriction base="xsd:int">
					<xsd:minInclusive value="20"/>
					<xsd:maxInclusive value="100000000"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-accounting-all">
		<xsd:sequence>
			<xsd:element name="AH-DELTA-ASSISTANT" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="primary" type="aaa:radius-accounting-server" minOccurs="0"/>
			<xsd:element name="backup1" type="aaa:radius-accounting-server" minOccurs="0"/>
			<xsd:element name="backup2" type="aaa:radius-accounting-server" minOccurs="0"/>
			<xsd:element name="backup3" type="aaa:radius-accounting-server" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="mac-delimiter-value">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="colon"/>
			<xsd:enumeration value="dash"/>
			<xsd:enumeration value="dot"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="mac-style-value">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="no-delimiter"/>
			<xsd:enumeration value="two-delimiter"/>
			<xsd:enumeration value="five-delimiter"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="mac-case-sensitivity-value">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="lower-case"/>
			<xsd:enumeration value="upper-case"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="open-directory-user">
		<xsd:sequence>
			<xsd:element name="password" type="general:ah-encrypted-string"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="optional"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="db-server-binddn">
		<xsd:sequence>
			<xsd:element name="password" type="general:ah-encrypted-string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="open-directory-domain">
		<xsd:sequence>
			<xsd:element name="fullname" type="general:ah-string" minOccurs="0"/>
			<xsd:element name="binddn" type="aaa:db-server-binddn" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="open-directory-server">
		<xsd:sequence>
			<xsd:element name="admin-user" type="aaa:open-directory-user" minOccurs="0"/>
			<xsd:element name="tls-enable" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="domain" type="aaa:open-directory-domain" minOccurs="0"/>
			<xsd:element name="filter-attr" type="general:ah-string-act" minOccurs="0"/>
			<xsd:element name="no-strip-filter" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-proxy-server">
		<xsd:sequence>
			<xsd:element name="shared-secret" type="general:ah-encrypted-string" minOccurs="0"/>
			<xsd:element name="tls" type="xsd:string" fixed="" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="xsd:string" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="local-radius-server">
		<xsd:sequence>
			<xsd:element name="server" type="aaa:radius-proxy-server" minOccurs="0"/>
			<xsd:element name="auth-port" type="aaa:aaa-auth-port" minOccurs="0"/>
			<xsd:element name="acct-port" type="aaa:aaa-acct-port" minOccurs="0"/>
			<xsd:element name="tls-port" type="aaa:radius-tls-port" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act-value" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="radius-proxy-dead-time-value">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="30"/>
			<xsd:maxInclusive value="3600"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="radius-proxy-dead-time">
		<xsd:attribute name="value" type="aaa:radius-proxy-dead-time-value" use="optional" default="300"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="radius-proxy-retry-count-value">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="1"/>
			<xsd:maxInclusive value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="radius-proxy-retry-count">
		<xsd:attribute name="value" type="aaa:radius-proxy-retry-count-value" use="optional" default="3"/>
	</xsd:complexType>
	<xsd:simpleType name="radius-proxy-retry-delay-value">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="3"/>
			<xsd:maxInclusive value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="radius-proxy-retry-delay">
		<xsd:sequence>
			<xsd:element name="retry-count" type="aaa:radius-proxy-retry-count" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="value" type="aaa:radius-proxy-retry-delay-value" use="optional" default="5"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="radius-realm-format-value">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="nai"/>
			<xsd:enumeration value="nt-domain"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="radius-realm-format">
		<xsd:attribute name="value" type="aaa:radius-realm-format-value" use="optional" default="nai"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-proxy-realm">
		<xsd:sequence>
			<xsd:element name="primary" type="general:ah-name-act" minOccurs="0"/>
			<xsd:element name="backup" type="general:ah-name-act" minOccurs="0"/>
			<xsd:element name="no-strip" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act-value" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-proxy-realm-all">
		<xsd:sequence>
			<xsd:element name="AH-DELTA-ASSISTANT" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="format" type="aaa:radius-realm-format" minOccurs="0"/>
			<xsd:element name="cr" type="aaa:radius-proxy-realm" minOccurs="0" maxOccurs="8"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="radsec-realm">
		<xsd:sequence>
			<xsd:element name="primary" type="general:ah-name-act" minOccurs="0"/>
			<xsd:element name="backup" type="general:ah-name-act" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act-value" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="radius-proxy-radsec">
		<xsd:sequence>
			<xsd:element name="AH-DELTA-ASSISTANT" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="auth-port" type="aaa:aaa-auth-port" minOccurs="0"/>
			<xsd:element name="acct-port" type="aaa:aaa-acct-port" minOccurs="0"/>
			<xsd:element name="realm" type="aaa:radsec-realm" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="tls-port" type="aaa:radius-tls-port" minOccurs="0"/>
			<xsd:element name="dynamic-auth-extension" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="enable" type="general:ah-only-act" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="radius-proxy">
		<xsd:sequence>
