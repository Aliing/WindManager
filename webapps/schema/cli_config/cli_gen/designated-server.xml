<?xml version="1.0" encoding="UTF-8"?>
<clis xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../src/xml/cli_config.xsd">

	<cli-gen key="designated-server.idm-proxy.dynamic">
		<constraints>
			<expression><![CDATA[ :v>=5.1.3.0 && :v<6.2.1.0  ]]></expression>
		</constraints>
		<cmd><![CDATA[ {false:no|true:} designated-server idm-proxy dynamic ]]></cmd>
		<cli-default value="{false}"/>
	</cli-gen>
	
	<cli-gen key="designated-server.idm-proxy.announce">
		<constraints>
			<version><![CDATA[ >=5.1.3.0 ]]></version>
		</constraints>
		<cmd><![CDATA[ {false:no|true:} designated-server idm-proxy announce ]]></cmd>
		<cli-default value="{false}"/>
	</cli-gen>
</clis>