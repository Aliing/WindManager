<?xml version="1.0" encoding="UTF-8"?>
<clis xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../src/xml/cli_config.xsd">

	<cli-gen key="ssid.user-profile-deny.action.strict">
		<constraints>
			<version>3.5.1.0-</version>
		</constraints>
		<cmd><![CDATA[ ssid <string> user-profile-deny action {1:ban|2:ban-forever|3:disconnect} [ {true:strict|false:} ] ]]></cmd>
		<cli-default value="{*, 3, false}"/>
	</cli-gen>
	
	<cli-gen key="ssid.user-profile-deny.action.ban.time">
		<constraints>
			<version>3.5.1.0-</version>
		</constraints>
		<cmd><![CDATA[ ssid <string> user-profile-deny action ban [ <number> ] ]]></cmd>
	</cli-gen>

</clis>