			<xsd:element name="mode" type="radio:band-steering-mode" minOccurs="0"/>
			<xsd:element name="prefer-5g" type="radio:band-steering-mode-prefer-5g" minOccurs="0"/>
			<xsd:element name="balance-band" type="radio:band-steering-mode-balance-band" minOccurs="0"/>
