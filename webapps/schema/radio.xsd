	<xsd:simpleType name="channel-width-value">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="20"/>
			<xsd:enumeration value="40-above"/>
			<xsd:enumeration value="40-below"/>
			<xsd:enumeration value="40"/>
			<xsd:enumeration value="80"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="chains-num-value">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="1"/>
			<xsd:maxInclusive value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="deny-client-value">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="11abg"/>
			<xsd:enumeration value="11b"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="interference-switch-iu-threshold-value">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="10"/>
			<xsd:maxInclusive value="80"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="interference-switch-iu-threshold">
		<xsd:attribute name="value" type="radio:interference-switch-iu-threshold-value" default="25"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="interference-switch-crc-err-threshold-value">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="10"/>
			<xsd:maxInclusive value="80"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="interference-switch-crc-err-threshold">
		<xsd:attribute name="value" type="radio:interference-switch-crc-err-threshold-value" default="25"/>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="acsp-interference-switch">
		<xsd:sequence>
			<xsd:element name="AH-DELTA-ASSISTANT" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="iu-threshold" type="radio:interference-switch-iu-threshold" minOccurs="0"/>
			<xsd:element name="crc-err-threshold" type="radio:interference-switch-crc-err-threshold" minOccurs="0"/>
			<xsd:choice minOccurs="0">
				<xsd:element name="enable" type="general:ah-act-show" minOccurs="0"/>
				<xsd:element name="disable" type="general:ah-act-show" minOccurs="0"/>
				<xsd:element name="no-station-enable" type="general:ah-act-show" minOccurs="0"/>
			</xsd:choice>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="acsp-access">
		<xsd:sequence>
			<xsd:element name="AH-DELTA-ASSISTANT" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="channel-auto-select" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="time-range" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="station" minOccurs="0">
										<xsd:complexType>
											<xsd:attribute name="value" use="optional" default="0">
												<xsd:simpleType>
													<xsd:restriction base="xsd:int">
														<xsd:minInclusive value="0"/>
														<xsd:maxInclusive value="100"/>
													</xsd:restriction>
												</xsd:simpleType>
											</xsd:attribute>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
								<xsd:attribute name="value" type="xsd:string" use="required"/>
								<xsd:attribute name="quoteProhibited" type="general:ah-enum-act" use="required"/>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
					<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="dfs" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="interference-switch" type="radio:acsp-interference-switch" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="wmm-ac-type">
		<xsd:sequence>
			<xsd:element name="AH-DELTA-ASSISTANT" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="aifs" minOccurs="0">
				<xsd:complexType>
					<xsd:attribute name="value" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:int">
								<xsd:minInclusive value="0"/>
								<xsd:maxInclusive value="15"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="cwmax" minOccurs="0">
				<xsd:complexType>
					<xsd:attribute name="value" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:int">
								<xsd:minInclusive value="1"/>
								<xsd:maxInclusive value="15"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="cwmin" minOccurs="0">
				<xsd:complexType>
					<xsd:attribute name="value" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:int">
								<xsd:minInclusive value="1"/>
								<xsd:maxInclusive value="15"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="noack" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="txoplimit" minOccurs="0">
				<xsd:complexType>
					<xsd:attribute name="value" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:int">
								<xsd:minInclusive value="0"/>
								<xsd:maxInclusive value="8192"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="channel-model-detail">
		<xsd:sequence minOccurs="0">
			<xsd:element name="cr" type="general:ah-string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="operation" type="general:ah-enum-act" use="required"/>
	</xsd:complexType>
	<xsd:simpleType name="crc-err-threshold-value">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="15"/>
			<xsd:maxInclusive value="60"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="cu-threshold-value">
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="15"/>
			<xsd:maxInclusive value="60"/>
		</xsd:restriction>
	</xsd:simpleType>
