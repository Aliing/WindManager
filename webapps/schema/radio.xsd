		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="5"/>
			<xsd:maxInclusive value="30"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="rate-health-percent">
		<xsd:sequence>
			<xsd:element name="success" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="usage" minOccurs="0">
							<xsd:complexType>
								<xsd:attribute name="value" type="general:ah-percent-value"/>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
					<xsd:attribute name="value" type="general:ah-percent-value"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required"/>
		<xsd:attribute name="operation" type="general:ah-enum-act-value" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="phymode-rate">
		<xsd:sequence>
			<xsd:element name="AH-DELTA-ASSISTANT" type="general:ah-only-act" minOccurs="0"/>
			<xsd:element name="rate" type="radio:rate-health-percent" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
