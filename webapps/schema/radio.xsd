		<xsd:sequence>
			<xsd:element name="_11a" type="radio:phymode-rate" minOccurs="0"/>
			<xsd:element name="_11b" type="radio:phymode-rate" minOccurs="0"/>
			<xsd:element name="_11g" type="radio:phymode-rate" minOccurs="0"/>
